{
  "name": "setup-issue-agent",
  "version": "1.0.0",
  "description": "GitHub Action For Issue Agent",
  "main": "index.js",
  "scripts": {
    "build": "npm run format:write && ncc build src/index.ts -o dist",
    "ci-test": "NODE_OPTIONS=--experimental-vm-modules NODE_NO_WARNINGS=1 npx jest",
    "format:write": "npx prettier --write .",
    "format:check": "npx prettier --check .",
    "lint": "npx eslint .",
    "local-action": "npx local-action . src/main.ts .env",
    "test": "NODE_OPTIONS=--experimental-vm-modules NODE_NO_WARNINGS=1 npx jest",
    "all": "npm run format:write && npm run lint && npm run test && npm run coverage && npm run build"
  },
  "author": "clover0",
  "license": "",
  "dependencies": {
    "@actions/cache": "4.0.0",
    "@actions/core": "1.11.1",
    "@actions/tool-cache": "2.0.2",
    "@octokit/rest": "21.1.1"
  },
  "devDependencies": {
    "@eslint/compat": "1.2.5",
    "@github/local-action": "2.5.0",
    "@jest/globals": "29.7.0",
    "@types/jest": "29.5.14",
    "@types/node": "20.17.12",
    "@typescript-eslint/eslint-plugin": "8.19.1",
    "@typescript-eslint/parser": "8.19.1",
    "@vercel/ncc": "0.38.3",
    "eslint": "9.18.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jest": "28.10.0",
    "eslint-plugin-prettier": "5.2.1",
    "jest": "29.7.0",
    "prettier": "3.4.2",
    "prettier-eslint": "16.3.0",
    "ts-jest": "29.2.5",
    "ts-jest-resolver": "2.0.1",
    "ts-node": "10.9.2",
    "typescript": "5.7.3"
  }
}
